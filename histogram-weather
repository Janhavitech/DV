import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_csv("weatherHistory.csv")

plt.figure(figsize=(8, 6))
plt.hist(df['Humidity'], bins=20, color='skyblue', edgecolor='black')
plt.title('Distribution of Humidity')
plt.xlabel('Humidity (%)')
plt.ylabel('Frequency')
plt.grid(True)
plt.tight_layout()
plt.show()
